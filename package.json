{
  "name": "flow-presentation",
  "version": "1.0.0",
  "description": "Sample app for flow presentation",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server",
    "flow": "flow",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
